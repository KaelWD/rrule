{
  "name": "rrule",
  "version": "2.8.0",
  "description": "JavaScript library for working with recurrence rules for calendar dates.",
  "homepage": "http://jakubroztocil.github.io/rrule/",
  "license": "BSD-3-Clause",
  "keywords": [
    "dates",
    "recurrences",
    "calendar",
    "icalendar",
    "rfc"
  ],
  "author": "Jakub Roztocil, Lars Sch√∂ning, and David Golightly",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakubroztocil/rrule.git"
  },
  "scripts": {
    "prepublish": "pnpm build",
    "prebuild": "pnpm lint && pnpm format-check",
    "build": "tsup",
    "lint": "pnpm eslint . --fix --config .eslintrc.js",
    "format": "pnpm prettier --write .",
    "format-check": "pnpm prettier --check .",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test-ci": "vitest run --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/mockdate": "^3.0.0",
    "@types/node": "^22.4.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "mockdate": "^3.0.5",
    "prettier": "^2.6.2",
    "tsup": "^8.2.4",
    "typescript": "^4.7.3",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "packageManager": "pnpm@9.7.1"
}
